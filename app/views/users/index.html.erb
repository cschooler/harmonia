<h1>This is the users home page</h1>

<ul class="users">
<% @users.each do | user |%>
<li class="user">
	<%= link_to 'Go', {:controller => "users", :action => "show", :id => user}, :class => 'title' %>
	<span><%= user.last_name %>, <%= user.first_name %></span>: <div><%= user.email %></div><div><%= user.aliases[0].alias %></div>
</li>
<% end%>
</ul>