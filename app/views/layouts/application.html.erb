<!DOCTYPE html>
<html>
	<head>
		<title><%= title %></title>
		<%= csrf_meta_tag %>
		<%= stylesheet_link_tag 'harmonia_base', :media => 'screen' %>
	</head>
	<body>
		<%= link_to "Logo Here", {:controller => "events", :action => "index"}, :id => 'site_logo' %>
		<h1 id="branding">Kauffman Center for the Performing Arts</h1>
		<div>
			<% flash.each do |key, value| %>
				<div class="flash <%= key %>"><%= value %></div>
			<% end %>
			<%= yield%>	
		</div>
		<%= debug(params) if Rails.env.development? %>
	</body>
</html>